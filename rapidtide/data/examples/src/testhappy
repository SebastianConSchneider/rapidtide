#!/bin/csh -f

happy \
    happyfmri.nii.gz \
    happyfmri.json \
    ../dst/happy_noflip \
    --mklthreads=8 \
    --dodlfilter \
    --saveinfoasjson \
    --model=model_revised \
    --aliasedcorrelation \
    --glm

happy \
    happyfmri.nii.gz \
    happyfmri.json \
    ../dst/happy_flip \
    --mklthreads=8 \
    --dodlfilter \
    --saveinfoasjson \
    --model=model_revised \
    --fliparteries \
    --aliasedcorrelation \
    --glm
