#!/bin/csh -f

happyx \
    happyfmri.nii.gz \
    happyfmri.json \
    ../dst/happyx_firstpass \
    --dodlfilter \
    --mklthreads=4 \
    --saveinfoasjson \
    --model=model_revised

happyx \
    happyfmri.nii.gz \
    happyfmri.json \
    ../dst/happyx_secondpass \
    --dodlfilter \
    --mklthreads=4 \
    --saveinfoasjson \
    --model=model_revised \
    --cardcalconly --estmask=../dst/happyx_firstpass_vesselmask.nii.gz


