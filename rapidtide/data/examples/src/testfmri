#!/bin/csh -f

rapidtide2 \
	fmri.nii.gz \
	../dst/dgsr_2 \
        --limitoutput \
        -s 25.0 \
        -L \
	-r -20,20 \
	-f 2 \
        --refinepasses=3 \
        --despecklepasses=4 \
        --accheck \
        --nprocs=-1 \
        --saveoptionsasjson \
        --detrendorder=3

rapidtide2x \
	fmri.nii.gz \
	../dst/dgsr \
        --limitoutput \
        -s 25.0 \
        -N 10000 \
        -L \
	-r -20,20 \
	-f 2 \
        --passes=3 \
        --refineoffset \
        --despecklepasses=4 \
        --nprocs=-1 \
        --saveoptionsasjson \
        --pickleft \
        --nofitfilt \
        --detrendorder=3

rapidtide2x \
	fmri_demeaned.nii.gz \
	../dst/dgsr_demeaned \
        --limitoutput \
        -s 25.0 \
        -N 0 \
        -L \
	-r -20,20 \
	-f 2 \
        --passes=3 \
        --refineoffset \
        --despecklepasses=4 \
        --nprocs=-1 \
        --saveoptionsasjson \
        --pickleft \
        --nofitfilt \
        --detrendorder=3

