#!/bin/csh -f

rapidtide \
	fmri.nii.gz \
	../dst/dgsr_quad \
        --delaymapping \
        --sigmalimit 25.0 \
        --ampthresh 0.3 \
        --nprocs -1 \
        --pickleft \
        --pickleftthresh 0.3 \
        --corrrefinetype quad \
	--spatialfilt 2 

#rapidtide2x \
	#fmri.nii.gz \
	#../dst/dgsr_2x \
        #--limitoutput \
        #-s 25.0 \
        #--ampthresh=0.3 \
        #-L \
	#-r -10,30 \
	#-f 2 \
        #--passes=3 \
        #--refineoffset \
        #--despecklepasses=4 \
        #--nprocs=-1 \
        #--pickleftthresh=0.3 \
        #--saveoptionsasjson \
        #--pickleft \
        #--noglm \
        #--detrendorder=3

diffmaps ../dst/dgsr_quad ../dst/dgsr ../dst/diff
