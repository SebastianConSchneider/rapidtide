#!/bin/csh -f
rapidtide2x \
	fmri.nii.gz \
	../dst/dgsr_rapidtide2x_multiproc_cubic \
        --limitoutput \
        -s 25.0 \
        -L \
	-r -20,20 \
	-f 2 \
        --refinepasses=3 \
        --despecklepasses=4 \
        --accheck \
        --nprocs=-1 \
        --detrendorder=3

