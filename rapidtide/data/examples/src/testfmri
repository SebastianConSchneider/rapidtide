#!/bin/csh -f
rapidtide2x \
	fmri.nii.gz \
	../dst/dgsr_maskcheck \
        --limitoutput \
        --lagminthresh=0.25 \
        --tmask=tmask.txt \
        -s 100.0 \
        -L \
	-r -20,20 \
	-f 2 \
        --refinepasses=3 \
        --despecklepasses=4 \
        --accheck \
        --includemask=fmri_mask.nii.gz \
        --excludemask=fmri_emptymask.nii.gz \
        --corrmask=fmri_mask.nii.gz \
        --nprocs=-1
